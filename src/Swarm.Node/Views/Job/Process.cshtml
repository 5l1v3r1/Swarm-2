<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        JOB PROCESSES
        <small>view job processes</small>
    </h1>
    <ol class="breadcrumb">
        <li>
            <a href="/"><i class="fa fa-dashboard"></i> Swarm</a>
        </li>
        <li>Job</li>
        <li class="active">Process</li>
    </ol>
</section>
<!-- Main content -->
<section class="content container-fluid" id="view">
    <div class="col-xs-12">
        <div class="box">
            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding">
                <table class="table table-bordered">
                    <tbody>
                    <tr>
                        <th>Name</th>
                        <th>Group</th>
                        <th>TraceId</th>
                        <th>Sharding</th>
                        <th>ProcessId</th>
                        <th>State</th>
                        <th>App</th>
                        <th>AppArguments</th>
                        <th>Msg</th>
                        <th>Time</th>
                    </tr>
                    <tr v-show="els.length==0">
                        <td colspan="13">
                            No data to display...
                        </td>
                    </tr>
                    <tr v-for="el in els">
                        <td>
                            <span v-text="el.name"></span>
                        </td>
                        <td>
                            <span v-text="el.group"></span>
                        </td>
                        <td>
                            <span v-text="el.traceId"></span>
                        </td>
                        <td>
                            <span v-text="el.sharding"></span>
                        </td>
                        <td>
                            <span v-text="el.processId"></span>
                        </td>
                        <td>
                            <span v-text="el.state"></span>
                        </td>
                        <td>
                            <span v-text="el.app"></span>
                        </td>
                        <td>
                            <span v-text="el.arguments"></span>
                        </td>
                        <td>
                            <span v-text="el.msg"></span>
                        </td>
                        <td>
                            <span v-text="hub.formatDate(el.lastModificationTime)"></span>
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>
            <!-- /.box-body -->
            <div class="box-footer clearfix">
                <ul id="pagination" v-show="total>size" class="pagination pagination-sm no-margin pull-left">                    
                </ul>
            </div>
        </div>
        <!-- /.box -->
    </div>
</section>
@section scripts{
    <script src="~/js/jobProcess.js" asp-append-version="true"></script>
}